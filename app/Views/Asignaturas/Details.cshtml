@model App.Models.Asignatura

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Asignatura</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.C贸digo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.C贸digo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        <table class="table">
            <thead class="thead-dark">
                <tr>
                <th>C贸digo</th>
                <th>Nombre</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Proyectos) {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.C贸digo)</td>
                    <td>@Html.DisplayFor(modelItem => item.Nombre)</td>
                </tr>
                }
            </tbody>
        </table>

    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Editar" asp-route-id="@Model.Id">Editar</a>
    <a class="btn btn-primary" asp-action="Index">Volver a la lista</a>
</div>
